<div class="container">
  <div class="row">
    <div class="col s3 m3">
      <button class="btn-floating btn-large waves-effect waves-light red lighten-2" {{bind-attr disabled="prevButton"}} {{action
'prevYear'}}>
          <i class="mdi-hardware-keyboard-arrow-left"></i>
      </button>
    </div>

    <div class="col s6 m6 center year-pagination">
      <h4>Year {{currentYear}} of {{totalYears}}</h4>
    </div>

    <div class="col s3 m3">
      <button class="btn-floating btn-large waves-effect waves-light red lighten-2 right" {{bind-attr disabled="nextButton"}} {{action
'nextYear'}}>
        <i class="mdi-hardware-keyboard-arrow-right"></i>
      </button>
    </div>

  </div>

  {{ember-chart type='line' data=chartData width=1200 height=600}}

</div>
